,UAAA,KAAA;AAAA,eAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA;AAAA,OAFA;AAGA;AAVA,GAdA;AA0BA,EAAA,OA1BA,qBA0BA;AACA,SAAA,EAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA;AACA,SAAA,aAAA;AACA,SAAA,UAAA;AACA,GA9BA;AA+BA,EAAA,QAAA,EAAA;AACA,IAAA,SAAA,EAAA,qBAAA;AACA,UAAA,CAAA;AACA,UAAA,GAAA,GAAA,EAAA;AACA,UAAA,CAAA;;AACA,UAAA,KAAA,MAAA,CAAA,MAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AAAA,QAAA,CAAA,GAAA,CAAA;AAAA,OAAA,MACA;AAAA,QAAA,CAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,MAAA;AAAA;;AACA,WAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,CAAA,EAAA,CAAA,EAAA,EAAA;AACA,QAAA,GAAA,GAAA,GAAA,GAAA,GAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,CAAA,EAAA,IAAA;AACA;;AACA,aAAA,GAAA;AACA,KAXA;AAYA,IAAA,KAAA,EAAA,iBAAA;AACA,UAAA,KAAA;AACA,UAAA,MAAA;AACA,MAAA,KAAA,GAAA,IAAA,CAAA,KAAA,CAAA,KAAA,MAAA,CAAA,OAAA,GAAA,EAAA,CAAA;AACA,MAAA,MAAA,GAAA,CAAA,KAAA,MAAA,CAAA,OAAA,GAAA,KAAA,GAAA,EAAA,IAAA,EAAA;AACA,MAAA,MAAA,GAAA,UAAA,CAAA,MAAA,CAAA,CAAA,OAAA,CAAA,CAAA,CAAA;AACA,MAAA,KAAA,GAAA,KAAA,GAAA,MAAA;AACA,aAAA,KAAA;AACA;AApBA;AA/BA,CAAA","sourcesContent":["<template>\n    \n    <div class=\"detail-film\">\n      <MenuBarre/>\n      <div class=\"cadre-film\">\n          <h1>{{ detail.title }}</h1>\n          <p>{{ trailer.results.id }}</p>\n          <div class=\"detail\">\n              <i v-if=\"detail.adult == adult\">Publics averti</i><i v-else>Tous publics</i><i class=\"vl\"></i>\n              <i>{{ detail.runtime }} minutes</i><i class=\"vl\"></i>\n              <i>{{ genrefilm }}</i><i class=\"vl\"></i>\n              <i>{{ detail.release_date }}</i>\n          </div>\n          <img v-bind:src=\"'http://image.tmdb.org/t/p/w500/' + detail.poster_path\">\n          <iframe v-if=\"trailer.results.key == ''\" v-bind:src=\"'https://www.youtube.com/embed/'+ trailer.results[0].key\"></iframe>\n          <iframe v-else v-bind:src=\"'https://www.youtube.com/embed/'+ trailer.results.key\"></iframe>\n          \n      </div>\n      \n      <div class=\"information-comp\">\n        <h2>SYNOPSIS</h2>\n        <p class=\"resumer-film\">{{ detail.overview }}</p>\n      </div>\n    </div>\n</template>\n\n<script>\nimport axios from 'axios'\nimport MenuBarre from './MenuBarre'\n\n\nexport default {\n  name: 'App',\n  components: {\n    MenuBarre\n  }, \n  data: () => {\n    return {\n      id: 0,\n      detail: '',\n      adult: 'adult',\n      trailer: ''\n    }\n  },\n  \n  methods: {\n    getDetailFilm() {\n      axios.get('https://api.themoviedb.org/3/movie/'+this.id+'?api_key=1a260002e6b25560efb27f6c948b8e62&language=fr-FR')   \n      .then(detail => (this.detail = detail.data))\n      .catch(error => console.log(error))\n    },\n    getTrailer(){\n      axios.get('https://api.themoviedb.org/3/movie/'+this.id+'/videos?api_key=1a260002e6b25560efb27f6c948b8e62&language=fr-FR')   \n      .then(trailer => (this.trailer = trailer.data))\n      .catch(error => console.log(error))\n    }\n  },\n    created() {\n        this.id = this.$route.params.id;\n        this.getDetailFilm(); \n        this.getTrailer();\n    },\n    computed: {\n      genrefilm: function () { \n        var i;\n        var res = '';\n        var b;\n        if(this.detail.genres.length > 3){b = 3}\n        else{b = this.detail.genres.length}\n        for(i=0;i< b;i++){\n          res = res + ' ' + this.detail.genres[i].name;\n        }\n        return res;\n      },\n      temps: function () {\n        var heure;\n        var minute;\n        heure = Math.floor(this.detail.runtime / 60);\n        minute = ((this.detail.runtime - (heure*60)))/60;\n        minute = parseFloat(minute).toFixed(2)\n        heure = heure + minute\n        return heure;\n      }\n      \n  }\n}\n</script>\n\n<style>\n.cadre-film{\n    font-family: 'comfortaa';\n    position: absolute;\n    background-color: white;color: black;border: solid orange;\n    width: 70%;\n    height: 37em;\n    text-align: center;\n    left: 13%;\n}\n.cadre-film img{\n    position: absolute;\n    width: 28%;\n    bottom: 0px;\n    left: 0;\n}\n.cadre-film i{\n    color: grey;\n}\n\n.cadre-film .vl{\n  border-left: 4px solid orange;\n  margin-left: 15px;\n  margin-right: 15px;\n  margin-top: 10px;\n}\n.cadre-film iframe{\n  position: absolute;\n  right: 0;\n  bottom: 0;\n  width: 70%;\n  height: 71%;\n}\n\n.resumer-film{\n  width: 90%;\n}\n\n.information-comp{\n  font-family: 'comfortaa';\n  left: 10%;\n  position: absolute;\n  top: 90%;\n}\n\n\n</style>\n"],"sourceRoot":"src/components"}]}